{
    "version": 2,
    "builds": [
      {
        "src": "main.py",
        "use": "@vercel/python"
      }
    ],
    "routes": [
      {
        "src": "/static/(.*)",
        "dest": "/static/$1"
      },
      {
        "src": "/(.*).ico",
        "dest": "/$1.ico"
      },
      {
        "src": "/.*",
        "dest": "main.py"
      }
    ]
  }
  